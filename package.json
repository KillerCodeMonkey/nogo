{
    "name": "nogo",
    "description": "A nodejs, mongodb and pm2 based rest api with token authorization",
    "author": "Bengt Wei√üe <bengtler@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/KillerCodeMonkey/nogo"
    },
    "keywords": [
        "nodejs",
        "cron",
        "email",
        "rest",
        "api",
        "mongodb",
        "bearer",
        "token authentication",
        "jwt",
        "jsonwebtoken"
    ],
    "scripts": {
        "start": "node index",
        "test": "node_modules/.bin/mocha tests/*",
        "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha tests/*",
        "docs": "node_modules/.bin/apidoc -i endpoints/ -o apiDoc/"
    },
    "dependencies": {
        "fs-extra": "0.26.2",
        "mongodb": "2.0.49",
        "express": "4.13.3",
        "body-parser": "1.14.1",
        "method-override": "2.3.5",
        "errorhandler": "1.4.2",
        "bluebird": "3.0.5",
        "mongoose": "4.2.7",
        "gulp": "3.9.0",
        "apidoc": "0.13.1",
        "jsonwebtoken": "5.4.1",
        "underscore": "1.8.3",
        "cors": "2.7.1",
        "nodemailer": "1.10.0",
        "nodemailer-smtp-transport": "1.0.4",
        "email-templates": "2.0.1",
        "handlebars": "4.0.5",
        "multer": "1.1.0"
    },
    "licence": "MIT",
    "devDependencies": {
        "proxyquire": "1.7.3",
        "form-data": "1.0.0-rc3",
        "expect.js": "0.3.1",
        "supertest": "1.1.0",
        "istanbul": "0.4.0",
        "mocha": "2.3.4"
    },
    "engines": {
        "node":  ">= 0.12.8"
    },
    "apidoc": {
        "title": "NoGo - API Documentation",
        "url" : "https://host/api/v1",
        "apiSampleRequest": "on",
        "sampleUrl": "localhost:8000/api/v1"
    }
}
